<?xml version="1.0" encoding="UTF-8"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
    <file source-language="en" target-language="de" datatype="plaintext"
          original="EXT:powermail_setup.git/Resources/Private/Language/locallang.xlf" date="2023-02-13T11:55:00Z"
          product-name="powermail_setup">
        <header/>
        <body>
            <trans-unit id="form.requirement.hint" resname="form.requirement.hint">
                <source>All fields marked "*" must be completed.</source>
                <target>Alle mit "*" gekennzeichneten Felder müssen ausgefüllt werden.</target>
            </trans-unit>
            <trans-unit id="form.navigation.label" resname="form.navigation.label">
                <source>Form steps</source>
                <target>Formularschritte</target>
            </trans-unit>
            <trans-unit id="form.errors.list.headline" resname="form.errors.list.headline">
                <source>Form errors</source>
                <target>Formularfehler</target>
            </trans-unit>
            <trans-unit id="form.errors.list.description" resname="form.errors.list.description">
                <source>There were %s errors when filling out the form. Please fix them and resubmit the corrected
                    form.
                </source>
                <target>Beim Ausfüllen des Formulars gab es %s Fehler. Bitte beheben Sie diese und senden Sie das
                    korrigierte Formular erneut ab.
                </target>
            </trans-unit>
            <trans-unit id="form.errors.list.description.single" resname="form.errors.list.description.single">
                <source>There were an error when filling out the form. Please fix it and resubmit the corrected form.
                </source>
                <target>Beim Ausfüllen des Formulars trat ein Fehler auf. Bitte beheben Sie diesen und senden Sie das
                    korrigierte Formular erneut ab.
                </target>
            </trans-unit>
            <trans-unit id="form.steps.next.label" resname="form.steps.next.label">
                <source>Next step: %s</source>
                <target>Nächster Schritt: %s</target>
            </trans-unit>
            <trans-unit id="form.steps.prev.label" resname="form.steps.prev.label">
                <source>Previous step: %s</source>
                <target>Vorheriger Schritt: %s</target>
            </trans-unit>

            <!-- UNUSED -->

            <trans-unit id="field.select.option" resname="field.select.option">
                <source>Please choose %s</source>
                <target>Bitte %s wählen</target>
            </trans-unit>
            <trans-unit id="form.errors.rendering.headline" resname="form.errors.rendering.headline">
                <source>Configuration error</source>
                <target>Konfigurationsfehler</target>
            </trans-unit>
            <trans-unit id="form.errors.rendering.message.requiredAttribute"
                        resname="form.errors.rendering.message.requiredAttribute">
                <source>Configuration parameter "%s" is missing but required. Please adjust your field configuration.
                </source>
                <target>Der Konfigurationsparameter "%s" fehlt, ist aber erforderlich. Bitte passen Sie Ihre
                    Feldkonfiguration an.
                </target>
            </trans-unit>
            <trans-unit id="form.elementsGroup.mandatory" resname="form.elementsGroup.mandatory">
                <source>One of the following options must be selected.</source>
                <target>Eine der folgenden Optionen muss ausgewählt werden.</target>
            </trans-unit>
        </body>
    </file>
