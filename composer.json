{
	"name" : "creifenscheid/powermail-setup",
	"type" : "typo3-cms-extension",
	"description" : "Extension to configure and set up EXT:powermail",
	"license" : "GPL-2.0-or-later",
	"require" : {
		"typo3/cms-core" : "^12.4",
		"in2code/powermail" : "^12.1"
	},
	"require-dev": {
		"roave/security-advisories": "dev-latest"
	},
	"autoload" : {
		"psr-4" : {
			"CReifenscheid\\PowermailSetup\\" : "Classes/"
		}
	},
	"replace": {
		"typo3-ter/powermail-setup": "self.version"
	},
	"minimum-stability" : "stable",
	"authors" : [{
		"name" : "C. Reifenscheid",
		"email" : "dev@christian-reifenscheid.de"
	}
	],
	"extra": {
		"typo3/cms": {
			"extension-key": "powermail_setup"
		}
	}
}
